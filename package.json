{
  "name": "webapp-boilerplate",
  "version": "1.2.0",
  "main": "index.js",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean && npm run link-helpers",
    "build:clean": "rimraf ./build",
    "link-helpers": "npm run link-helpers:clean && ln -Ffs $(pwd)/helpers $(pwd)/src/helpers && ln -Ffs $(pwd)/helpers $(pwd)/server/helpers",
    "link-helpers:clean": "rm -f $(pwd)/src/helpers $(pwd)/server/helpers",
    "dev": "redis-server & cross-env NODE_ENV=development NODE_PATH=./server SECRET_API_KEY=asdfghjkl BUILT_AT=$( date -u +%s ) nodemon --ignore 'server/server_content/*' --ignore 'src/*' --ignore 'build/*' --exec babel-node --inspect ./server/app.js src",
    "build": "npm run build:client && npm run build:server",
    "build:client": "next build",
    "build:server": "cross-env babel -d ./build/server ./server -s --copy-files && cp next.config.js ./build",
    "start": "cross-env NODE_PATH=./build/server BUILT_AT=$( date -u +%s ) NODE_ENV=production node ./build/server/app.js",
    "start:aws": "npm run start",
    "test": "cross-env jest --coverage",
    "test:watch": "cross-env jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src --fix && eslint server --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --syntax scss",
    "prettier": "prettier --config .prettierrc \"./**/*.+(js|json|jsx)\" --write",
    "clean:example": "rnb --c",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "./**/*.+(js|json|jsx)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "async-lock": "^1.2.4",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.14.0",
    "detect-browser": "^5.1.1",
    "express": "^4.17.1",
    "gg-components": "^10.0.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "^7.0.5",
    "immutable": "^4.0.0-rc.12",
    "invariant": "2.2.4",
    "ip": "^1.1.5",
    "ip-parse": "^1.0.5",
    "lodash": "^4.17.15",
    "next": "^9.4.4",
    "next-images": "^1.4.0",
    "next-transpile-modules": "^3.3.0",
    "nodemailer": "^6.4.8",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-modal": "^3.11.2",
    "react-redux": "7.0.2",
    "redis": "^3.0.2",
    "redis-mock": "^0.49.0",
    "redux": "4.0.5",
    "redux-saga": "1.0.2",
    "redux-saga-routines": "^3.1.3",
    "reselect": "4.0.0",
    "safe-compare": "^1.1.4",
    "sass": "^1.26.9",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@next/bundle-analyzer": "^9.4.4",
    "@r-next-b/cli": "^0.1.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-redux-saga": "^1.0.0",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-slow-down": "^1.3.1",
    "helmet": "^3.23.3",
    "husky": "^2.4.1",
    "jest-cli": "^24.9.0",
    "lint-staged": "^10.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "stylelint-config-skyscanner": "^2.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
