{
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    "babel-plugin-add-module-exports", 
    ["module-resolver", { 
      "root": ["./"],
      "alias": {
        "gg-webapp": "./gg-webapp",
        "helpers": "./shared/helpers",
        "server-utils": "./server/utils",
        "client-utils": "./src/utils"
      }
    }]
  ]
}
